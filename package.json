  },
  "dependencies": {
    "twilio": "^3.19.2"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "serverless": "^1.31.0",
    "serverless-domain-manager": "^2.6.5",
    "xml2js": "^0.4.19"
